{{#partial "contenido"}}
	<section>
		<form method="POST" action="/entidades/juridicas/new">
		<center> <div class = "w3-container">
			<h2> Cargar Entidad Juridica </h2>
			
			<br> <h3> Nombre Ficticio </h3>
			<input type="text" name = "nombreFicticio" placeholder = "Nombre Ficticio">
			
			<br> <h3> Razon Social </h3>
			<input type="text" name = "razonSocial" placeholder = "Razon Social">
			
			<br> <h3> CUIT </h3>
			<input type="number" name = "CUIT" placeholder = "CUIT">
			
			<br> <h3> Direccion </h3>
			<input type="text" name = "direccion" placeholder = "Direccion">
			
			<br> <h3> Cod. Inscripto </h3>
			<input type="number" name = "codInscripto" placeholder = "Cod. Inscripto">
			
			<br> <h3> Tipo </h3>
			<select name="Tipo">
   				<option selected value="0"> Elige una opcion </option>
       				<option value="EmpresaMicro">EmpresaMicro</option>
       				<option value="EmpresaPequenia">EmpresaPequenia</option>
       				<option value="EmpresaMedianaTramo1">EmpresaMedianaTramo1</option>
       				<option value="EmpresaMedianaTramo2">EmpresaMedianaTramo2</option>
       				<option value="OSC">OSC</option>
   				</optgroup> 
			</select>
			 
			<br><br><a href="/entidades/new">Volver</a> &nbsp &nbsp &nbsp
			<input type = "submit" class="btn btn-primary" value="Guardar"/> <br> <br>
		</div> </center>
		</form>
	</section>

{{/partial}}

